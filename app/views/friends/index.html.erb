
<% if user_signed_in? %>

<table class="table">

<thead>
<tr>
<th>Name</th>
<th>Email</th>
<th>Phone</th>
<th>Twitter</th>

<th> </th>
</tr>
</thead> 

<tbody>

  <% @friends.each do |friend| %>
  <% if friend.user == current_user %>
  <tr>
<td>
      <%= link_to friend.first_name + " " + friend.last_name, friend %>

      
</td>

<td><%= friend.email %>
</td>
<td><%= friend.phone %>
</td>
<td><%= friend.twitter %>
</td>






<td><%= button_to "Delete", friend, method: :delete, data: { confirm: "Are you sure?" }, class: "btn btn-warning btn-sm" %></td>
</tr>
  <% end %>


<% end %>
</tbody>
</table>
<br>


<%= link_to "New friend", new_friend_path, class: "btn btn-secondary" %>

<% else %>


<h1>Hello World</h1>
This is my ruby 

<% end %>

